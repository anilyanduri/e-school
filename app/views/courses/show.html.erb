<div id="content">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="box">
                <p style="color: green"><%= notice %></p>
                <h3>Course</h3>
                <table class="table" id="schools">
                  <thead>
                    <tr>
                      <th scope="col">Name</th>
                      <th scope="col">Desctiption</th>
                      <th scope="col">Material</th>
                      <th scope="col">Active</th>
                      <th scope="col" colspan="3">Actions</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr id="<%= dom_id @course%>">
                      <%= render @course %>
                    </tr>
                    <% if Current.user.school_admin?(Current.school) %>
                    <tr>
                      <td colspan="4"></td>
                      <td colspan="3">
                        <%= link_to raw("<i class='fa fa-list-alt'></i> Courses"), admin_courses_path %>
                      </td>
                    </tr>
                    <%end%>
                  </tbody>
                </table>
                <h4>Students enroled to Course Admins</h4>
                <table class="table table-striped" id="users">
                  <thead>
                    <tr>
                      <th scope="col">First Name</th>
                      <th scope="col">Last Name</th>
                      <th scope="col">Email</th>
                      <th scope="col">Admin</th>
                      <th scope="col">Student</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @users.each do |user|%>
                    <tr id="<%= dom_id user%>">
                      <%= render partial: "admin/schools/user", locals: {user: user, course: @course} %>
                    </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>