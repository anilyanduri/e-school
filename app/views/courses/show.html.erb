      <div id="content">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="box">
                <h3>Course</h3>
                <table class="table" id="schools">
                  <thead>
                    <tr>
                      <th scope="col">Name</th>
                      <th scope="col">Desctiption</th>
                      <th scope="col">Material</th>
                      <th scope="col">Active</th>
                      <th scope="col" colspan="3">Actions</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr id="<%= dom_id @course%>">
                      <%= render @course %>
                    </tr>
                    <% if Current.user.school_admin?(Current.school) %>
                    <tr>
                      <td colspan="4"></td>
                      <td colspan="3">
                        <%= link_to raw("<i class='fa fa-list-alt'></i> Courses"), admin_courses_path %>
                      </td>
                    </tr>
                    <%end%>
                  </tbody>
                </table>
                <h4>Batches</h4>
                <table class="table table-striped" id="batches">
                  <thead>
                    <tr>
                      <th scope="col">Batch Name</th>
                      <th scope="col">Students enrolled</th>
                      <th scope="col">Status</th>
                      <th scope="col">Students Enroled</th>
                      <th scope="col" colspan="3">Actions</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @batches.each do |batch|%>
                    <tr id="<%= dom_id batch%>">
                      <%= render batch %>
                    </tr>
                    <% end %>
                    <% if Current.user.school_admin?(Current.school) %>
                    <tr>
                      <td colspan="4"></td>
                      <td colspan="2" class="text-end">
                        <%= link_to raw("<i class='fa fa-plus'></i> New Batch"), new_admin_batch_path %>
                      </td>
                      <td></td>
                    </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>